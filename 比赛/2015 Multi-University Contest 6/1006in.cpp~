#include <cstdio>
#include <cstring>
#include <iostream>
#include <algorithm>

using namespace std;


int main()
{
    cout << "1" << endl;
    cout << "100000"<<endl;
    for( int i = 1 ; i <= 100000 ; i++ )
        cout << 1 << " "<<endl;
    return 0;
}
